msgstr "błędna liczba bajtów do porównania"

#: src/uniq.c:645
#, c-format
msgid "--group is mutually exclusive with -c/-d/-D/-u"
msgstr "--group wyklucza się wzajemnie z -c/-d/-D/-u"

# ? rzm
