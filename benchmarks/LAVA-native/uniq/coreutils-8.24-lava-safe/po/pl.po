msgstr "wielokrotna specyfikacja pól"

#: src/numfmt.c:1677
#, c-format
msgid "invalid header value %s"
msgstr "błędna zawartość nagłówka %s"

#: src/numfmt.c:1704
#, c-format
msgid "--grouping cannot be combined with --format"
msgstr "opcja --grouping nie może być użyta razem z --format"

#: src/numfmt.c:1709
#, c-format
